{
  "manifest_version": 2,
  "name": "vndict",
  "short_name": "vndict",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
	"background": {
	  "scripts": ["background.js","constant.js"]
	},
	"content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["script-content.js","constant.js"],
    "css": ["styles.css"]
	}]
}
